# Copy this file to .env and fill in your values
# .env is gitignored and will not be committed

# OpenRouter API key for AIL tests (LLM agent testing)
OPENROUTER_API_KEY=your-openrouter-api-key

# Enable AIL tests (disabled by default)
AIL_TESTS=true

# Optional: Override default test database path
# AIL_TEST_DB=./db/test

# Optional: Override LLM model (default: anthropic/claude-haiku-4.5)
# AIL_MODEL=anthropic/claude-haiku-4.5

# Optional: Enable verbose logging
# AIL_VERBOSE=true
